<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="program.LoginPageController">
   <children>
      <AnchorPane prefHeight="600.0" prefWidth="800.0">
         <children>
            <Button fx:id="login_btn" layoutX="115.0" layoutY="362.0" mnemonicParsing="false" onAction="#handleGoLogin_btnOnAction" prefHeight="51.0" prefWidth="571.0" text="Login">
               <font>
                  <Font name="System Italic" size="23.0" />
               </font>
            </Button>
            <Label layoutX="126.0" layoutY="202.0" text="Username :">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Label layoutX="126.0" layoutY="295.0" text="Passwords :">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Button fx:id="cnac_btn" layoutX="115.0" layoutY="426.0" mnemonicParsing="false" onAction="#handleGoCnac_btnOnAction" style="-fx-background-color: transparent;" text="Create a new account">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <TextField layoutX="264.0" layoutY="195.0" prefHeight="51.0" prefWidth="422.0" promptText="input your username">
               <font>
                  <Font size="24.0" />
               </font>
            </TextField>
            <PasswordField layoutX="264.0" layoutY="285.0" prefHeight="51.0" prefWidth="422.0" promptText="input your passwords">
               <font>
                  <Font size="24.0" />
               </font>
            </PasswordField>
         </children></AnchorPane>
   </children>
</AnchorPane>
